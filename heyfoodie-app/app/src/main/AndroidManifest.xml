<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hayfoodie">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.hayfoodie.AddEmp"></activity>
        <activity android:name="com.example.hayfoodie.AddItem" />
        <activity android:name="com.example.hayfoodie.AddTable" />
        <activity android:name="com.example.hayfoodie.UpdateEmployee" />
        <activity android:name="com.example.hayfoodie.CustomerMenu" />
        <activity android:name="com.example.hayfoodie.AdminScreen" />
        <activity android:name="com.example.hayfoodie.Login" />
        <activity android:name="com.example.hayfoodie.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>